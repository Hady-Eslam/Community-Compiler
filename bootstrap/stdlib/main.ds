extern {
    fn printf(x: str);
}

fn foo(x: i32)
{
    printf("i32: %d\n", x);
}

fn foo(x: u8)
{
    printf("u8: %d\n", x);
}

fn foo(x: str)
{
    printf("str: %s\n", x);
}


fn bar() : u8
{
    50
}

fn qaz() : str
{
    "bob the builder"
}

fn main()
{
    printf("Hello World!\n");

    let x = 10;
    let y = 5u8;
    let z = bar();

    foo("test");
    foo(x);
    foo(y);
    foo(z);
    foo(qaz());
}