extern {
    fn printf(x: str);
    fn fopen(filename : str, mode : str) : u32;
    fn getc(stream : u32) : i32;
    fn fclose(stream : u32) : i32;
}

struct point
{   
    x: u32
    y: u32
    z: u32
}


fn main()
{
    var p = point(15, 5, 21);
    printf("x: %d, y: %d, z:%d\n", p.x, p.y, p.z);
    free(p);
}
